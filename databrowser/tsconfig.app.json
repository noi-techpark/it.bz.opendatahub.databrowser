{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "src/env.d.ts",
        "src/**/*",
        "src/**/*.vue"
    ],
    "exclude": [
        "src/**/__tests__/*"
    ],
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "lib": [
            "ESNext",
        ],
        "types": [
            "node"
        ],
        "verbatimModuleSyntax": false
    }
}